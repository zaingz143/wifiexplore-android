<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"


    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dell.wifiexplorer.Options">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </RelativeLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:layout_gravity="bottom"

        android:weightSum="100">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="11"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal"
            android:tileMode="repeat">



            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"

                android:layout_marginRight="15dp"
                android:background="@drawable/headerp4"/>


        </RelativeLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1.5"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:tileMode="repeat">
            <View
                android:layout_width="match_parent"
                android:layout_height="0.7dp"
                android:background="@android:color/white"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="8"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:layout_marginTop="1dp"
            android:orientation="horizontal"
            android:weightSum="20"
            >


            <ImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/router"

                android:layout_weight="3"
                android:id="@+id/imageView" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="ATNT"

                android:layout_weight="18"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="6dp"
                android:textSize="24sp"
                android:layout_marginRight="50dp"
                android:textColor="#ffffff"

                android:id="@+id/name" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="10"

            android:orientation="horizontal"
            android:tileMode="repeat">

        </LinearLayout>


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="20"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:weightSum="20">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="8.6"
                android:layout_centerHorizontal="true"
                android:onClick="true"
                android:id="@+id/login"
                android:weightSum="20">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"


                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/circle75"
                        android:clickable="true"
                        android:src="@drawable/mycircledoller"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="75"
                        android:textSize="30dp"
                        android:textColor="#ffffff"

                        android:gravity="center"/>
                </RelativeLayout>

                <View
                    android:id="@+id/verticalSeperatorHours5"
                    android:layout_gravity="center"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    />
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:clickable="true"


                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/key2"
                        android:src="@drawable/mycirclestar"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="4/5"
                        android:textSize="30dp"
                        android:textColor="#ffffff"
                        android:gravity="center"/>
                </RelativeLayout>




            </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:weightSum="20">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="8.6"
                android:layout_centerHorizontal="true"
                android:onClick="true"
                android:id="@+id/login32"
                android:weightSum="20">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"


                    >


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="TOTAL EARNING"
                        android:textSize="12dp"
                        android:textColor="#ffffff"

                        android:gravity="top|center"/>
                </RelativeLayout>

                <View
                    android:id="@+id/verticalSeperatorHours7"
                    android:layout_gravity="center"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    />
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:clickable="true"


                    >


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="RATING"
                        android:textSize="12dp"
                        android:textColor="#ffffff"
                        android:gravity="top|center"/>
                </RelativeLayout>




            </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal"
            android:tileMode="repeat">
            <View
                android:layout_width="match_parent"
                android:layout_height="0.7dp"
                android:background="@android:color/white"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="20"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:weightSum="20">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="8.6"
                android:layout_centerHorizontal="true"
                android:onClick="true"
                android:id="@+id/login43"
                android:weightSum="20">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"


                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/circle7455"
                        android:clickable="true"
                        android:src="@drawable/mycircledateuse"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="234578"
                        android:textSize="17dp"
                        android:textColor="#ffffff"

                        android:gravity="center"/>
                </RelativeLayout>

                <View
                    android:id="@+id/verticalSeperatorHours435"
                    android:layout_gravity="center"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    />
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:clickable="true"
                    android:weightSum="10"

                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/key3"
                        android:src="@drawable/mycircle"
                        android:paddingRight="10dp"
                        android:paddingLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        />
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/key5"
                        android:src="@drawable/nofuser"
                        android:paddingRight="40dp"
                        android:paddingLeft="40dp"
                        android:layout_marginTop="35dp"
                        android:layout_marginBottom="35dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="3"
                        android:textSize="17dp"
                        android:textColor="#ffffff"
                        android:layout_marginLeft="65dp"
                        android:layout_marginTop="13dp"
                        android:gravity="top|center"/>

                </RelativeLayout>





            </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:weightSum="20">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="8.6"
                android:layout_centerHorizontal="true"
                android:onClick="true"
                android:id="@+id/login3432"
                android:weightSum="20">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"


                    >


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="DATA USAGE"
                        android:textSize="12dp"
                        android:textColor="#ffffff"

                        android:gravity="top|center"/>
                </RelativeLayout>

                <View
                    android:id="@+id/verticalSeperatorHours547"
                    android:layout_gravity="center"
                    android:layout_width="1dip"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    />
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:clickable="true"


                    >


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="NO OF USERS"
                        android:textSize="12dp"
                        android:textColor="#ffffff"
                        android:gravity="top|center"/>
                </RelativeLayout>




            </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="10"

            android:orientation="horizontal"
            android:tileMode="repeat">
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="10"
            android:orientation="horizontal"

            android:background="#4f070707"
            android:weightSum="10">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/edit"
                android:layout_weight="2.5"
                android:id="@+id/editwifi"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.3"

                android:layout_gravity="center"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/del"
                android:layout_weight="2.5"
android:id="@+id/del"
                android:layout_gravity="center"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/time"
                android:layout_weight="2.8"
                android:id="@+id/his"
                android:layout_gravity="center"
                />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>

